from utils.Binning import load_distribution
import sys


if __name__=="__main__":
    dist=load_distribution(sys.stdin)
    print '%s\t%s'%(dist.get_mean(), dist.get_real_std_dev())
    print "percentile: 5, 25, 50, 75 and 95"
    print '%s'%('\t'.join(["%s"%value  for value in dist.get_percentiles([5,25,50,75,95])]))
    print "nb of entry with at least: 1, 5, 10, 20 and 50"
    print '%s'%('\t'.join(["%s"%value  for value in dist.get_greater_or_equal_than([1,5,10,20,50])]))
    