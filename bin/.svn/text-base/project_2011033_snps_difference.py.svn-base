import sys, os
from IO_interface import vcfIO


if __name__=='__main__':
    """Get the snps present in High not in Low and the other way around"""
    input_file=sys.argv[1]
    open_input = open(os.path.abspath(input_file))
    reader = vcfIO.VcfReader(open_input)
    for record in reader:
        valid_genotypes = record.get_valid_genotype_per_sample(genotype_quality_threshold=20)
        if len(valid_genotypes.keys())==2:
            genotype1,genotype2 = valid_genotypes.keys() 
            if len(set(genotype1.split('/')))==1 and len(set(genotype2.split('/')))==1:
                print record
    open_input.close()
